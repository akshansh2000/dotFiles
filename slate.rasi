* {
  background-color: #262626;
  border-color: #2e343f;
  text-color: #8ca0aa;
  spacing: 0;
  width: 300px;
}

inputbar {
  border: 0 0 1px 0;
  children: [prompt,entry];
}

prompt {
  padding: 16px;
  border: 0 1px 0 0;
}

textbox {
  background-colo: #2e343f;
  border: 0 0 1px 0;
  border-color: #282C33;
  padding: 8px 16px;
}

entry {
  padding: 16px;
}

listview {
  cycle: true;
  margin: 0 0 -1px 0;
  scrollbar: false;
  lines: 8;
}

element {
  border: 0 0 1px 0;
  padding: 16px;
}

element selected {
  background-color: #3c3836;
}
